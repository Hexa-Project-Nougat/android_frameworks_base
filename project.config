[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/KangDroid (KangDroid)
[access "refs/for/refs/*"]
	read = group user/KangDroid (KangDroid)
	push = group user/KangDroid (KangDroid)
	label-Code-Review = -2..+2 group user/KangDroid (KangDroid)
	label-Verified = -1..+1 group user/KangDroid (KangDroid)
	rebase = group user/KangDroid (KangDroid)
	removeReviewer = group user/KangDroid (KangDroid)
	submit = group user/KangDroid (KangDroid)
[access "refs/heads/*"]
	read = group user/KangDroid (KangDroid)
	create = group user/KangDroid (KangDroid)
	push = +force group user/KangDroid (KangDroid)
	pushMerge = group user/KangDroid (KangDroid)
[access "refs/tags/*"]
	push = +force group user/KangDroid (KangDroid)
	pushTag = group user/KangDroid (KangDroid)
	pushSignedTag = group user/KangDroid (KangDroid)
